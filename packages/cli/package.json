{
  "name": "@logvault/cli",
  "version": "0.2.1",
  "description": "LogVault CLI - Audit logging setup in 2 minutes",
  "type": "module",
  "bin": {
    "logvault": "./dist/bin/logvault.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/bin/logvault.js",
    "test": "echo 'No tests yet'",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "logvault",
    "audit",
    "cli",
    "logging",
    "compliance",
    "soc2",
    "gdpr",
    "security"
  ],
