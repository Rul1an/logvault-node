{
  "name": "@logvault/eslint-plugin",
  "version": "0.2.4",
  "description": "ESLint plugin for audit gap detection - Shift-Left Compliance",
  "keywords": [
    "eslint",
    "eslintplugin",
    "audit",
    "logging",
    "compliance",
    "security",
    "logvault"
  ],
  "author": "LogVault",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Rul1an/logvault-node.git",
    "directory": "packages/eslint-plugin"
  },
  "homepage": "https://github.com/Rul1an/logvault-node/tree/main/packages/eslint-plugin#readme",
  "bugs": {
    "url": "https://github.com/Rul1an/logvault-node/issues"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "docs"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src tests",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "@typescript-eslint/utils": "^8.16.0"
  },
  "devDependencies": {
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.10.1",
    "@typescript-eslint/rule-tester": "^8.16.0",
    "eslint": "^9.15.0",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "peerDependencies": {
    "eslint": ">=8.57.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
